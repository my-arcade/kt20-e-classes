{
  "compileOnSave": false,
  "compilerOptions": {
      "allowSyntheticDefaultImports": true,
      "lib": [
          "dom",
          "es2015",
          "es2016"
      ],
      "jsx": "react",
      "target": "es2016",
      "module": "esnext",
      "moduleResolution": "node",
      "noImplicitAny": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "removeComments": false,
      "preserveConstEnums": true,
      "sourceMap": true,
      "skipLibCheck": true,
      "resolveJsonModule": true,
      "baseUrl": "./",
      "paths": {
        "@models": ["./../models/src"],
        "@ui": ["./../app-ui/src"],
        "@modules": ["./src/modules"],
        "@form": ["./../app-ui/src/core/Form"]
      }
  },
  "files": ["./../app-ui/src/core/theme/styled.d.ts"]
}